{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "compile-space-validate-export": "ts-node --files --project space-data/tsconfig.json space-data/src/compile_space.ts -i space-data/content -o public/build/content",
    "compile-space-jsonschema": "eslint space-data && typescript-json-schema space-data/schema/schema.ts --required SpaceObject > space-data/schema/schema.json",
    "compile-space-netlifycmsschema": "ts-node --project space-data/tsconfig.json space-data/src/js2netcms.ts",
    "compile-space-full": "run-s compile-space-jsonschema compile-space-netlifycmsschema compile-space-validate-export",
    "build:rollup": "rollup -c",
    "build": "run-s compile-space-full test build:rollup",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "postinstall": "husky install",
    "test:eslint": "eslint .",
    "test:jest": "jest",
    "test:svelte-check": "svelte-check",
    "test": "run-p test:*"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "@types/hammerjs": "^2.0.36",
    "@types/jest": "^26.0.20",
    "@types/js-yaml": "^4.0.0",
    "@types/json-schema": "^7.0.7",
    "@types/marked": "^1.2.1",
    "@types/node": "^14.14.22",
    "@types/rbush": "^3.0.0",
    "@types/sanitize-html": "^1.27.1",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.14.2",
    "chalk": "^4.1.0",
    "commander": "^7.0.0",
    "eslint": "^7.17.0",
    "hammerjs": "^2.0.8",
    "husky": "^5.0.6",
    "jest": "^26.6.3",
    "js-yaml": "^4.0.0",
    "marked": "^1.2.7",
    "npm-run-all": "^4.1.5",
    "rbush": "^3.0.1",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sanitize-html": "^2.3.1",
    "svelte": "^3.31.2",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.6.1",
    "three": "^0.124.0",
    "tinyqueue": "^2.0.3",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "tslib": "^2.0.0",
    "tslog": "^3.1.0",
    "typescript": "^3.9.7",
    "typescript-json-schema": "^0.47.0",
    "vscode-json-languageservice": "^4.0.0"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0"
  },
  "jest": {
    "preset": "ts-jest"
  }
}
